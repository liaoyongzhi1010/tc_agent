# TC Agent 配置

# 服务配置
TC_AGENT_HOST=127.0.0.1
TC_AGENT_PORT=8765
TC_AGENT_DEBUG=false

# LLM配置
TC_AGENT_LLM_PROVIDER=qwen
TC_AGENT_LLM_MODEL=

# API Keys (至少配置一个)
TC_AGENT_QWEN_API_KEY=your_qwen_api_key
TC_AGENT_ZHIPU_API_KEY=your_zhipu_api_key
TC_AGENT_DOUBAO_API_KEY=your_doubao_api_key
TC_AGENT_DOUBAO_ENDPOINT_ID=your_endpoint_id

# Embedding配置
TC_AGENT_EMBEDDING_MODE=local
TC_AGENT_EMBEDDING_MODEL=BAAI/bge-small-zh-v1.5

# RAG配置
TC_AGENT_RAG_CHILD_CHUNK_SIZE=200
TC_AGENT_RAG_PARENT_CHUNK_SIZE=1000
TC_AGENT_RAG_TOP_K=5

# 工具包
TC_AGENT_TOOL_PACKS=core,runner

# 后端工作区与Runner
# 建议设置为 Docker 可共享目录（如 Linux: /home/<user>/code/...）
TC_AGENT_WORKSPACE_ROOT=/tmp/tc_agent_workspaces
TC_AGENT_OPTEE_IMAGE=tc-agent/optee-build:4.0
TC_AGENT_RUNNER_BACKEND=inline  # inline | redis
TC_AGENT_RUNNER_QUEUE_KEY=tc_agent:queue:runner
TC_AGENT_RUNNER_POLL_INTERVAL=1.0
TC_AGENT_RUNNER_TTL=86400

# 工作流存储
TC_AGENT_WORKFLOW_STORE=memory  # memory | redis
TC_AGENT_REDIS_URL=redis://localhost:6379/0
TC_AGENT_WORKFLOW_TTL=86400

# Agent配置
TC_AGENT_AGENT_MAX_ITERATIONS=30
